
entity Transcript {
    entrezGeneId Integer required,
    hugoSymbol String required,
    referenceGenome ReferenceGenome required,
    ensemblTranscriptId String,
    ensemblProteinId String,
    referenceSequenceId String,
    description String
}

entity GenomeFragment {
    chromosome String,
    start Integer,
    end Integer,
    strand Integer,
    type GenomeFragmentType
}

enum GenomeFragmentType {
    GENE,
    EXON,
    FIVE_PRIME_UTR,
    THREE_PRIME_UTR
}

entity Sequence {
    sequenceType SequenceType
    sequence TextBlob
}

entity TranscriptUsage {
	source UsageSource
}

enum SequenceType {
	PROTEIN, CDNA
}
enum ReferenceGenome {
    GRCh37, GRCh38
}

enum UsageSource {
 	ONCOKB
}

entity Drug {
    name TextBlob
    code String
    semanticType TextBlob
}

entity DrugSynonym {
    name TextBlob
}

entity Info {
    type InfoType unique required
    value String
    lastUpdated Instant
}

enum InfoType {
    NCIT_VERSION,
    GENE_LAST_UPDATED
}

entity Gene {
    entrezGeneId Integer
    hugoSymbol String
}

entity GeneAlias {
    name String
}

relationship OneToMany {
	Drug{synonyms} to DrugSynonym
	Gene to GeneAlias
	Transcript{fragments} to GenomeFragment{transcript}
}

relationship ManyToOne {
	TranscriptUsage to Transcript
    Sequence to Transcript
}

service * with serviceClass
dto Transcript with mapstruct
skipClient *

